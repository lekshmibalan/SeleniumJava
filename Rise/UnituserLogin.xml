<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
		<listener class-name="generatereport.Listeners">
		</listener>
	</listeners>
		<parameter name="browser" value="chrome"></parameter>
	<test name="unitlogin">
		<classes>
			<class name="pagestestscript.LoginPageTest">
				<methods>
					<include
						name="verifyThatUserIsAbleToLoginAsPublicUserWithValidCredentials"></include>
					<include
						name="verifyThatUserIsAbleToLoginAsPublicUserWithInvalidCredentials"></include>
					<include
						name="verifyThatUserisAbleToLoginAsPublicUserWithoutChanginguserType"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="opening unit management module in public user">
		<classes>
			<class name="pagestestscript.ModuleListPageTest"></class>
		</classes>
	</test>
</suite>