<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="generatereport.Listeners"></listener>
	</listeners>
	<parameter name="browser" value="chrome"></parameter>
	<test name="loginpage">
		<classes>
			<class name="pagestestscript.LoginPageTest">
				<methods>
					<include name="verifyThatTheUserIsAbleToLoginAsSeepzUser"></include>
					<include
						name="verifyThatTheUserIsAbleToLoginAsSeepzUserWithInvalidCredentials"></include>
					<include
						name="verifyThatTheUserIsAbleToLoginWithInvalidUsenameAndValidPassword"></include>
					<include
						name="verifyThatTheUserIsAbleToLoginWithValidUserNameAndInvalidPassword"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="open UAC module">
		<classes>
			<class name="pagestestscript.ModuleListPageTest">
				<methods>
					<include name="verifyThatTheUserIsAbleToClickOn_UAC_Module"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>